file db/I404.template
# Macro - Description
# DEVICE - Device name
# PORT - Asyn portname
# CAP0 - Value for capacitor 0 (normally 100pF)
# CAP1 - Value for capacitor 1 (normally 3300pF)
#
{
pattern 
{    Sys,         Dev    ,    PORT, CAP0 , CAP1   }
{ "XF:09IDB-BI", "{i404:1}",   COM1, 100pF, 3300pF }
}
